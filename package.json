{
  "name": "ebook-reader-js",
  "version": "1.0.0",
  "description": "ebook reader",
  "author": "Alen Toma",
  "license": "MIT",
  "repository": "Alen Toma/ebook-reader-js",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --tsconfig tsconfig.json --compress --css-modules false --format modern,cjs",
    "buildWithFonts": "copyfiles -u 2 src/book_fonts/* dist/book_fonts && npm run build",
    "start": "microbundle-crl watch --tsconfig tsconfig.json --no-compress --css-modules false --format modern,cjs",
    "ready": "npm run build && npm run copyjavascript",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "single": "webpack --config webpack.config.js"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.180",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-ranger": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "base64-inline-loader": "^2.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "gulp": "^4.0.2",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^3.9.10",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@alentoma/usestate": "^1.2.5",
    "advanced-html-parser": "^1.1.0",
    "keen-slider": "^6.6.4",
    "lodash": "^4.17.21",
    "react": "^16.14.0",
    "react-ranger": "^2.1.0",
    "react-text-selection-popover": "^2.0.1",
    "react-native-webview": "11.17.2"
  }
}
